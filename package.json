{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm ./api/index.ts",
    "devs": "npm run build && npm run watch",
    "prepare": "husky",
    "build": "webpack --mode=production",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "watch": "nodemon --watch 'api/' --ext 'ts,json' --exec 'node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm ./api/index.ts'",
    "tsc": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "lint": "eslint --fix",
    "format": "prettier --write",
    "build:back": "babel ./api/**/*.ts --out-file dist/index.mjs --extensions '.ts' --ignore '**/*.test.ts' --presets=@babel/preset-env --plugins=@babel/plugin-transform-modules-umd"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@jest/globals": "^29.7.0",
    "@stylistic/eslint-plugin-ts": "^1.8.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.8",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "commitlint": "^19.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "style-loader": "^4.0.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "ws": "^8.17.0"
  }
}
